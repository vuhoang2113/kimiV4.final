{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "clean_public": "rm -rf ./public/css && rm -rf ./public/images && rm -rf ./public/js",
        "clean_storage": "rm -f ./public/storage && rm -rf ./storage/app/public",
        "clean_node_modules": "rm -rf ./node_modules",
        "clean": "npm run clean_public && npm run clean_storage && npm run clean_node_modules"
    },
